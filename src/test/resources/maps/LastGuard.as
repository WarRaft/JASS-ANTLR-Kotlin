			StunUnitTimed(Picked, stuntime);
		}
		GroupRemoveUnit(gmain, Picked);
	}
	SaveInteger(udg_hash, h, 1, amount - 6);
	if (amount < -1) {
		DestroyTimer(t);
		FlushChildHashtable(udg_hash, h);
	else
		TimerStart(t, .75, false, Trig_water_explosion_periodic_Actions);
