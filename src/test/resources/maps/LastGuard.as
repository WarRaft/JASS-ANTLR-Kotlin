	float x1U;
	float y1U;
	float d1UUF;
	float X = GetUnitX(Unit);
	float Y = GetUnitY(Unit);
	while (true) {
		ufil[FirstOfGroup(g)] = FirstOfGroup(g);
		if (ufil == null) break;
