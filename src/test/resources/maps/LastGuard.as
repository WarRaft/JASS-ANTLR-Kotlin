			}
			DestroyEffect(AddSpecialEffect("Abilities\\Spells\\Human\\MarkOfChaos\\MarkOfChaosTarget.mdl", GetUnitX(u[lop]), GetUnitY(u[lop])));
			DestroyLightning(l);
			FlushChildHashtable(udg_hash, GetHandleId(u[lop]));
			u[lop] = nil;
			l = nil;
