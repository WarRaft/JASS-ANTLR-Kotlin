	ef = nil;
	Picked = nil;
	p = nil;
	GroupClear(gdamage);
	DestroyGroup(gdamage);
	gdamage = nil;
	GroupClear(gdamage2);
	DestroyGroup(gdamage2);
	gdamage2 = nil;
	dammi = nil;
	ef = nil;
