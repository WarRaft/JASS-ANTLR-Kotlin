			if (p ~= pmaxtakedamage) {
				percent = ((udg_Player_Taken_Damage_Current[GetPlayerId(p)] / alltakendamage) * 'd');
				DisplayTimedTextToPlayer(p, .0, .0, 20., "  : ( |cff99ccff" + I2S(R2I(udg_Player_Taken_Damage_Current[GetPlayerId(p)])) + "|r  - |cff99ccff" + (SubString(R2S(percent), 0, StringLength(R2S(percent)) - 2)) + "%|r )");
