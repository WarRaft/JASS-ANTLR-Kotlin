	local xorb ---@type float
	local yorb ---@type float
	local angle = bj_RADTODEG * Atan2((yt - y), (xt - x)) ---@type float
	local dist = SquareRoot(((xt - x) * (xt - x)) + ((yt - y) * (yt - y))) ---@type float
	local p ---@type player
	local g ---@type group
	local grup ---@type group
	local Picked ---@type unit
