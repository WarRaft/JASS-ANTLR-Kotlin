int CountItemOfTypeOnUnit(unit u, int itemId) {
	local index = 0 ---@type int
	local count = 0 ---@type int
	local indexItem ---@type item
	while (true) {
		indexItem = UnitItemInSlot(u, index);
		if ((indexItem ~= nil) and (GetItemTypeId(indexItem) == itemId)) {
			count = count + 1;
		}
		index = index + 1;
		if (index >= 6) break;
	}
	return count;
}
