	TimerStart(talive, 1., true, Trig_Start_Beorn_Den_Check_Alive_Units_Actions);
	Picked = nil;
	boss = nil;
	GroupClear(gruppa);
	DestroyGroup(gruppa);
	gruppa = nil;
	t3sec = nil;
