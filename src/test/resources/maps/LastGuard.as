	CinematicFadeBJ(0, 4., "ReplaceableTextures\\CameraMasks\\White_mask.blp", 0, 0, 0, 1);
	while (true) {
		Picked = FirstOfGroup(gruppa);
		if (Picked == nil) break;
		SetUnitPosition(Picked, 2090., (-8300. + dist));
		if (dist > .0) {
