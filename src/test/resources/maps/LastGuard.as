		DestroyEffect(AddSpecialEffectTarget("Abilities\\Spells\\Undead\\DeathPact\\DeathPactCaster.mdl", target, "origin"));
		while (true) {
			Picked = FirstOfGroup(gdamage);
			if (Picked == nil) break;
