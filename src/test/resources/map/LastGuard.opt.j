	local real dot2 = (ax_0 - cx_0) * (ax_0 - cx_0) + (ay_0 - cy_0) * (ay_0 - cy_0)
	local real dot3 = (px_0 - cx_0) * (bx_0 - cx_0) + (py_0 - cy_0) * (by_0 - cy_0)
